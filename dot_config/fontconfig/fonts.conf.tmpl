<?xml version="1.0"?>
<!DOCTYPE fontconfig SYSTEM "fonts.dtd">
<!-- vim: ft=xml
-->
<fontconfig>
    <alias>
        <family>serif</family>
        <prefer><family>{{ .normfont }}</family></prefer>
    </alias>
    <alias>
        <family>sans-serif</family>
        <prefer><family>{{ .normfont }}</family></prefer>
    </alias>
    <alias>
        <family>sans</family>
        <prefer><family>{{ .normfont }}</family></prefer>
    </alias>
    <alias>
        <family>monospace</family>
        <prefer><family>{{ .normfont }}</family></prefer>
    </alias>
</fontconfig>
